{
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "extends": [
    "standard"
  ],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly",
    "notice": "readonly",
    "$": "readonly",
    "pluginMaster": "readonly",
    "__profilesStore": "readonly"
  },
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "plugins": [
    "react",
    "@typescript-eslint"
  ],
  "rules": {
    "lines-between-class-members": "off",
    "promise/param-names": "off",
    "no-unused-vars": "off",
    "object-curly-newline": "off",
    "no-sequences": "off"
  }
}

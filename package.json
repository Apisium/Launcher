{
  "name": "pure-launcher",
  "private": true,
  "version": "0.0.0",
  "description": "An awesome Minecraft Launcher.",
  "main": "index.js",
  "scripts": {
    "lint": "eslint src/ --ext .js,.ts,.tsx",
    "lint:fix": "eslint src/ --fix --ext .js,.ts,.tsx",
    "start": "parcel index.html src/main.ts --target electron --public-url .",
    "run": "electron .",
    "watch": "parcel watch index.html src/main.ts --target electron --public-url .",
    "build": "del-cli dist && parcel build index.html --target electron --public-url . --bundle-node-modules --no-source-maps",
    "test": "echo \"Error: no test specified\" && exit 1",
    "compile": "npm run compile:cpp && npm run compile:rust",
    "compile:cpp": "electron-build-env --disturl http://npm.taobao.org/mirrors/atom-shell -- cmake-js compile",
    "compile:rust": "electron-build-env --disturl http://npm.taobao.org/mirrors/atom-shell -- neon build --release"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Apisium/PureLauncher.git"
  },
  "keywords": [
    "minecraft",
    "launcher",
    "electron",
    "TypeScript",
    "parcel",
    "react",
    "hooks"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "author": "Shirasawa",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Apisium/PureLauncher/issues"
  },
  "homepage": "https://github.com/Apisium/PureLauncher#readme",
  "devDependencies": {
    "@babel/core": "^7.7.2",
    "@babel/traverse": "^7.7.4",
    "@babel/types": "^7.7.4",
    "@types/fs-extra": "^8.0.1",
    "@types/koa": "^2.11.0",
    "@types/koa-router": "^7.0.42",
    "@types/lodash.merge": "^4.6.6",
    "@types/minimist": "^1.2.0",
    "@types/node": "^12.12.7",
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.4",
    "@types/react-router-dom": "^5.1.2",
    "@typescript-eslint/eslint-plugin": "^2.6.1",
    "@typescript-eslint/parser": "^2.6.1",
    "@xmcl/installer": "^1.0.1",
    "@xmcl/launch": "^1.0.2",
    "@xmcl/task": "^1.0.1",
    "@xmcl/version": "^1.0.1",
    "babel-plugin-module-resolver": "^3.2.0",
    "cmake-js": "^6.0.0",
    "del-cli": "^3.0.0",
    "electron": "^6.1.4",
    "electron-build-env": "^0.2.0",
    "eslint": "^6.6.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-standard": "^4.0.1",
    "framer-motion": "^1.6.15",
    "fs-extra": "^8.1.0",
    "husky": "^3.0.9",
    "koa": "^2.11.0",
    "koa-body": "^4.1.1",
    "koa-router": "^8.0.0-alpha.1",
    "less": "^3.10.3",
    "lodash.merge": "^4.6.2",
    "minimist": "^1.2.0",
    "moment": "^2.24.0",
    "node-addon-api": "^1.7.1",
    "p-all": "^2.1.0",
    "parcel": "^1.12.4",
    "rc-dialog": "^7.5.13",
    "rc-notification": "^3.3.1",
    "rc-tooltip": "^3.7.3",
    "react": "^16.12.0",
    "react-deep-force-update": "^2.1.3",
    "react-dom": "^16.11.0",
    "react-image": "^2.2.1",
    "react-live-route": "^3.1.6",
    "react-router-dom": "^5.1.2",
    "react-slick": "^0.25.2",
    "react-textfit": "^1.1.0",
    "react-treebeard": "^3.2.4",
    "reqwq": "^0.2.0",
    "skinview3d": "^1.2.0-alpha.1",
    "slick-carousel": "^1.8.1",
    "typescript": "^3.7.2",
    "use-react-router": "^1.0.7",
    "uuid-by-string": "^3.0.2"
  },
  "browserslist": [
    "electron 6.1.4"
  ],
  "gypfile": true,
  "cmake-js": {
    "runtime": "electron",
    "runtimeVersion": "6.1.4"
  }
}

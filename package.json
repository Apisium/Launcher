{
  "name": "pure-launcher",
  "private": true,
  "version": "0.0.0",
  "description": "An awesome Minecraft Launcher.",
  "main": "index.js",
  "scripts": {
    "lint": "eslint src/ --ext .js,.ts,.tsx",
    "lint:fix": "eslint src/ --fix --ext .js,.ts,.tsx",
    "start": "parcel index.html src/main.ts --no-autoinstall --target electron --public-url .",
    "run": "electron .",
    "watch": "parcel watch index.html src/main.ts --target electron --public-url . --no-autoinstall",
    "build": "del-cli dist && parcel build index.html src/main.ts --target electron --public-url . --bundle-node-modules --no-source-maps --no-cache",
    "test": "echo \"Error: no test specified\" && exit 1",
    "compile": "npm run compile:cpp && npm run compile:rust",
    "compile:cpp": "electron-build-env --disturl http://npm.taobao.org/mirrors/atom-shell -- cmake-js compile",
    "compile:rust": "electron-build-env --disturl http://npm.taobao.org/mirrors/atom-shell -- neon build --release"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Apisium/PureLauncher.git"
  },
  "keywords": [
    "minecraft",
    "launcher",
    "electron",
    "TypeScript",
    "parcel",
    "react",
    "hooks"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "author": "Shirasawa",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Apisium/PureLauncher/issues"
  },
  "homepage": "https://github.com/Apisium/PureLauncher#readme",
  "devDependencies": {
    "@babel/core": "^7.7.5",
    "@babel/traverse": "^7.7.4",
    "@babel/types": "^7.7.4",
    "@types/fs-extra": "^8.0.1",
    "@types/minimist": "^1.2.0",
    "@types/node": "^12.12.17",
    "@types/react": "^16.9.16",
    "@types/react-dom": "^16.9.4",
    "@types/react-router-dom": "^5.1.3",
    "@typescript-eslint/eslint-plugin": "^2.11.0",
    "@typescript-eslint/parser": "^2.11.0",
    "@xmcl/installer": "^1.0.4",
    "@xmcl/launch": "^1.1.0",
    "@xmcl/task": "^1.0.4",
    "@xmcl/version": "^1.0.4",
    "babel-plugin-dollar": "file:packages/babel-plugin-dollar",
    "babel-plugin-module-resolver": "^4.0.0",
    "del-cli": "^3.0.0",
    "electron": "^7.1.5",
    "eslint": "^6.7.2",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-standard": "^4.0.1",
    "framer-motion": "^1.7.0",
    "fs-extra": "^8.1.0",
    "husky": "^3.1.0",
    "less": "^3.10.3",
    "lodash": "^4.17.15",
    "minimist": "^1.2.0",
    "moment": "^2.24.0",
    "p-all": "^2.1.0",
    "parcel": "^1.12.4",
    "parcel-plugin-bundle-visualiser": "^1.2.0",
    "parcel-plugin-replacer": "file:packages/parcel-plugin-replacer",
    "rc-dialog": "^7.6.0",
    "rc-notification": "^3.3.1",
    "rc-tooltip": "^3.7.3",
    "react": "^16.12.0",
    "react-deep-force-update": "^2.1.3",
    "react-dom": "^16.12.0",
    "react-image": "^2.2.1",
    "react-live-route": "^3.1.6",
    "react-router-dom": "^5.1.2",
    "react-slick": "^0.25.2",
    "react-textfit": "^1.1.0",
    "react-treebeard": "^3.2.4",
    "reqwq": "^0.2.0",
    "skinview3d": "^1.2.0-alpha.1",
    "slick-carousel": "^1.8.1",
    "typescript": "^3.7.3",
    "use-react-router": "^1.0.7",
    "uuid-by-string": "^3.0.2"
  },
  "browserslist": [
    "electron 6.1.6"
  ],
  "gypfile": false,
  "cmake-js": {
    "runtime": "electron",
    "runtimeVersion": "7.1.5"
  },
  "files": [
    "src/**/*",
    "packages/**/*"
  ]
}

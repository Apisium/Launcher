{
  "name": "pure-launcher",
  "private": true,
  "version": "0.0.0",
  "description": "An awesome Minecraft Launcher.",
  "main": "index.js",
  "scripts": {
    "lint": "tslint --config tslint.json --project tsconfig.json",
    "lint:fix": "tslint --config tslint.json --project tsconfig.json --fix",
    "start": "parcel index.html --target electron --public-url .",
    "run": "electron .",
    "watch": "parcel watch index.html --target electron --public-url .",
    "build": "npm run lint && parcel build index.html --experimental-scope-hoisting --target electron --public-url . --no-hmr",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Apisium/PureLauncher.git"
  },
  "keywords": [
    "minecraft",
    "launcher",
    "electron",
    "TypeScript",
    "parcel",
    "react",
    "hooks"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "author": "Shirasawa",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Apisium/PureLauncher/issues"
  },
  "homepage": "https://github.com/Apisium/PureLauncher#readme",
  "devDependencies": {
    "@xmcl/launch": "0.0.23",
    "@types/fs-extra": "^8.0.0",
    "@types/lodash.merge": "^4.6.6",
    "@types/node": "^12.7.5",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "@types/react-router-dom": "^4.3.5",
    "electron": "^6.0.9",
    "framer-motion": "^1.6.9",
    "fs-extra": "^8.1.0",
    "husky": "^3.0.5",
    "less": "^3.10.3",
    "lodash.merge": "^4.6.2",
    "moment": "^2.24.0",
    "node-addon-api": "^1.7.1",
    "p-all": "^2.1.0",
    "parcel": "^1.12.3",
    "rc-dialog": "^7.5.7",
    "rc-notification": "^3.3.1",
    "rc-tooltip": "^3.7.3",
    "react": "^16.9.0",
    "react-deep-force-update": "^2.1.3",
    "react-dom": "^16.9.0",
    "react-image": "^2.2.0",
    "react-live-route": "^3.1.5",
    "react-router-dom": "^5.0.1",
    "react-slick": "^0.25.2",
    "skinview3d": "^1.2.0-alpha.1",
    "slick-carousel": "^1.8.1",
    "tslint": "^5.20.0",
    "tslint-config-standard": "^8.0.1",
    "tslint-react": "^4.1.0",
    "typescript": "^3.6.3",
    "use-model": "git+https://github.com/ShirasawaSama/use-model.git",
    "use-react-router": "^1.0.7",
    "uuid-by-string": "^2.1.5"
  },
  "browserslist": [
    "electron 6.0.9"
  ],
  "gypfile": true,
  "dependencies": {
  }
}
